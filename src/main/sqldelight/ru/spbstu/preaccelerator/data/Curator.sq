import dev.inmo.tgbotapi.types.UserId;

CREATE TABLE Curator (
  userId BIGINT AS UserId PRIMARY KEY
);

isEmpty:
SELECT NOT EXISTS (SELECT * FROM Curator);

get:
SELECT * FROM Curator
WHERE userId = :userId;

getCurators:
SELECT * FROM Curator;

add:
INSERT INTO Curator
VALUES(:userId)
ON CONFLICT(userId)
DO NOTHING;